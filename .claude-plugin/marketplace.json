{
  "name": "anthropic-agent-skills",
  "owner": {
    "name": "Keith Lazuka",
    "email": "klazuka@anthropic.com"
  },
  "metadata": {
    "description": "Anthropic example skills",
    "version": "1.0.0"
  },
  "plugins": [
    {
      "name": "document-skills",
      "description": "Collection of document processing suite including Excel, Word, PowerPoint, and PDF capabilities",
      "source": "./",
      "strict": false,
      "skills": [
        "./document-skills/xlsx",
        "./document-skills/docx",
        "./document-skills/pptx",
        "./document-skills/pdf"
      ]
    },
    {
      "name": "example-skills",
      "description": "Collection of example skills demonstrating various capabilities including skill creation, MCP building, visual design, algorithmic art, internal communications, web testing, artifact building, Slack GIFs, and theme styling",
      "source": "./",
      "strict": false,
      "skills": [
        "./canvas-design",
        "./algorithmic-art",
        "./internal-comms",
        "./webapp-testing",
        "./artifacts-builder",
        "./slack-gif-creator",
        "./theme-factory",
        "./brand-guidelines"
      ]
    },
    {
      "name": "developer-skills",
      "description": "Collection of skills for developers",
      "source": "./",
      "strict": false,
      "skills": [
        "./skill-creator",
        "./mcp-builder",
        "./webapp-testing"
      ]
    },
    {
      "name": "starlims-skills",
      "description": "Collection of skills for Starlims",
      "source": "./",
      "strict": false,
      "skills": [
        "./xfd-coding",
        "./ssl-coding"
      ],
      "mcpServers": {
        "starlims-mcp-server": {
          "command": "starlims-mcp-server"
        }
      }
    },
    {
      "name": "hook-monitor",
      "description": "Monitor for hooks",
      "source": "./hook-monitor",
      "strict": false,
      "hooks": {
        "PreToolUse": [
          {
            "matcher": "*",
            "hooks": [
              {
                "type": "command",
                "command": "\"$CLAUDE_PLUGIN_ROOT\"/hook_handler.py"
              }
            ]
          }
        ],
        "PostToolUse": [
          {
            "matcher": "*",
            "hooks": [
              {
                "type": "command",
                "command": "\"$CLAUDE_PLUGIN_ROOT\"/hook_handler.py"
              }
            ]
          }
        ],
        "UserPromptSubmit": [
          {
            "hooks": [
              {
                "type": "command",
                "command": "\"$CLAUDE_PLUGIN_ROOT\"/hook_handler.py"
              }
            ]
          }
        ],
        "Notification": [
          {
            "hooks": [
              {
                "type": "command",
                "command": "\"$CLAUDE_PLUGIN_ROOT\"/hook_handler.py"
              }
            ]
          }
        ],
        "Stop": [
          {
            "hooks": [
              {
                "type": "command",
                "command": "\"$CLAUDE_PLUGIN_ROOT\"/hook_handler.py"
              }
            ]
          }
        ],
        "SubagentStop": [
          {
            "hooks": [
              {
                "type": "command",
                "command": "\"$CLAUDE_PLUGIN_ROOT\"/hook_handler.py"
              }
            ]
          }
        ],
        "PreCompact": [
          {
            "hooks": [
              {
                "type": "command",
                "command": "\"$CLAUDE_PLUGIN_ROOT\"/hook_handler.py"
              }
            ]
          }
        ],
        "SessionStart": [
          {
            "hooks": [
              {
                "type": "command",
                "command": "\"$CLAUDE_PLUGIN_ROOT\"/hook_handler.py"
              }
            ]
          }
        ],
        "SessionEnd": [
          {
            "hooks": [
              {
                "type": "command",
                "command": "\"$CLAUDE_PLUGIN_ROOT\"/hook_handler.py"
              }
            ]
          }
        ]
      }
    }
  ]
}
